{
  "name": "sscrg-app-frontend",
  "version": "0.0.1",
  "description": "A web application that aims to help SSCRG's members.",
  "main": "index.js",
  "scripts": {
    "create": "webpack",
    "start": "webpack-dev-server --mode development --open",
    "build": "set NODE_ENV=production & webpack --mode=production -p",
    "test": "jest",
    "testCoverage": "jest --coverage --runInBand",
    "testWatch": "jest --watch",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls"
  },
  "keywords": [
    "SSCRG",
    "Web Application",
    "react",
    "pwa"
  ],
  "author": "Kevin W.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PengWang0316/SSCRGApp-Frontend.git"
  },
  "sideEffects": false,
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "e2e/*"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/app/tools/assetsTransformer.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/app/tools/setupTests.js"
    ],
    "coveragePathIgnorePatterns": [
      "app/resources/",
      "app/tools",
      "index.js"
    ],
    "testEnvironment": "jest-environment-jsdom-global",
    "testURL": "http://localhost",
    "verbose": true
  },
  "dependencies": {
    "@kevinwang0316/i18n": "^1.0.0",
    "@kevinwang0316/react-materialui-pagination": "^1.1.4",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "aws-amplify": "^1.1.22",
    "aws-amplify-react": "^2.3.2",
    "axios": "^0.19.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-hot-loader": "^4.7.1",
    "react-imported-component": "^5.5.3",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "workbox-sw": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.3.4",
    "axios-mock-adapter": "^1.16.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "copy-webpack-plugin": "^5.0.0",
    "coveralls": "^3.0.3",
    "css-loader": "^2.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.1.0",
    "jest-environment-jsdom": "^24.0.0",
    "jest-environment-jsdom-global": "^1.1.1",
    "mini-css-extract-plugin": "^0.5.0",
    "raf": "^3.4.1",
    "react-test-renderer": "^16.8.3",
    "redux-mock-store": "^1.5.3",
    "sinon": "^7.2.6",
    "style-loader": "^0.23.1",
    "uglify-es": "^3.3.9",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "workbox-webpack-plugin": "^4.0.0"
  }
}
